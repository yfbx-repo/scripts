
#!/bin/bash

set -e

if [ -z $1 ]; then
    echo "command: key-info <keystore> <password>"
    return
fi

if [ -z $2 ]; then
    echo "command: key-info <keystore> <password>"
    return
fi


file=$1
password=$2

if [ -f $file ];then

    keytool -list -v -storepass $password -keystore $file | grep "MD5" -A 3

    echo "注意：百度地图和高的地图等平台取的是MD5值"
else 
    echo "$file is not found or invalid!"    
fi
